<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TablayoutTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="CustomRadioButton" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/custom_radiobutton</item>
        <item name="android:textColor">#C9D6CF</item>
        <item name="android:textColorSecondary">#000000</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="android:spinnerStyle">@style/customspinner</item>
    </style>
    <style name="customspinner" parent="@style/Widget.AppCompat.Spinner">
        <item name="android:textColor">@color/custom_color</item>
        <item name="android:background">@drawable/spinner_background</item>
    </style>

</resources>